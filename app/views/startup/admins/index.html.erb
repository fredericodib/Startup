<h1>Gestores da startup</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>email</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.name %></td>
        <td><%= admin.email %></td>
        <td><%= link_to 'Show', startup_admin_path(admin) %></td>
        <% if admin != current_admin %>
          <td><%= link_to 'Destroy', startup_admin_path(admin), method: :delete, data: { confirm: 'Tem certeza?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
